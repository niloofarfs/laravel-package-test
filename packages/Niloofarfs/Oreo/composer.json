{
    "name": "niloofarfs/oreo",
    "description": "User management for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Niloofar Fallahian",
            "email": "ns.falahian@gmail.com",
            "homepage": "https://github.com/niloofarfs"
        }
    ],
    "homepage": "https://github.com/niloofarfs/oreo",
    "keywords": ["Laravel", "Oreo"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Niloofarfs\\Oreo\\": "src/",
            "Niloofarfs\\Oreo\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Niloofarfs\\Oreo\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Niloofarfs\\Oreo\\OreoServiceProvider"
            ],
            "aliases": {
                "Oreo": "Niloofarfs\\Oreo\\Facades\\Oreo"
            }
        }
    }
}
